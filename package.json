{
  "name": "linkedin-autoconnect-chrome-extension",
  "version": "1.0.0",
  "description": "Automatically clicks all 'Connect' buttons available on LinkedIn 'Search People' and 'Recommended For You' pages.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felladrin/linkedin-autoconnect-chrome-extension.git"
  },
  "author": "Victor Nogueira <victor@victornogueira.app>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felladrin/linkedin-autoconnect-chrome-extension/issues"
  },
  "scripts": {
    "watch": "run-s recreate-dist-folder copy-static-files-to-dist && run-p vite-html-watch vite-lib-watch",
    "build": "run-s recreate-dist-folder copy-static-files-to-dist vite-html vite-lib zip-dist-folder",
    "vite-html": "vite build --config vite-pages-config.ts",
    "vite-lib": "vite build --config vite-tab-config.ts",
    "vite-html-watch": "npm run vite-html -- --watch --mode development",
    "vite-lib-watch": "npm run vite-lib -- --watch --mode development",
    "copy-static-files-to-dist": "shx cp -r \"static/*\" \"dist\"",
    "zip-dist-folder": "cd dist && bestzip linkedin-autoconnect-chrome-extension.zip *",
    "recreate-dist-folder": "shx rm -rf dist && shx mkdir dist"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.143",
    "@types/node": "^15.6.1",
    "@types/react-dom": "^17.0.5",
    "bestzip": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "typescript": "^4.3.2",
    "vite": "^2.3.4"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.6.3",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@types/react": "^17.0.8",
    "effector": "^21.8.11",
    "effector-react": "^21.3.2",
    "framer-motion": "^4.1.17",
    "is-what": "^4.0.0",
    "random-int": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0"
  }
}
